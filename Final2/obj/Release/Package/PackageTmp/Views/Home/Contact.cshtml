@model Final2.Models.EmailFormModel
@{
    ViewBag.Title = "Contact";
}
<body class="contact">
    
<br/>
    <a href="https://github.com/happypoofysquirrel" target="_blank"> <img src="~/Content/Images/icon_github.png" alt="Git Hub"/></a>
    <a href="https://www.linkedin.com/in/guyverhopkins" target="_blank"> <img src="~/Content/Images/101833-silver-inlay-square-metal-icon-social-media-logos-linkedin-logo.png" alt="Linkedin" height="190" width="190"/> </a>
      
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <h4>Whats on your mind?</h4>
            <hr />
            <div class="form-group">
                @Html.LabelFor(m => m.FromName, new { @class = "col-md-2 control-label" })
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.FromName, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.FromName)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.FromEmail, new { @class = "col-md-2 control-label" })
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.FromEmail, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.FromEmail)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.Subject, new { @class = "col-md-2 control-label" })
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.Subject, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.Subject)
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(m => m.Message, new { @class = "col-md-2 control-label" })
                <div class="col-md-12">
                    @Html.TextAreaFor(m => m.Message, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.Message)
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" class="btn btn-default" value="Send" />
                </div>
            </div>
            <div class="from-group">
                <div class="col-md-offset-6 col-md-6">

                </div>
            </div>
        }

</body>
